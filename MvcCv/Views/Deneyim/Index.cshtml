@using MvcCv.Models.Entity
@model IEnumerable<TblDeneyimler>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Deneyim listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt başlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
    <td>
        @item.ID
    </td> <td>
        @item.Baslik
    </td> <td>
        @item.AltBaslik
    </td> <td>
        @item.Tarih
    </td>

          <td><a href="/Deneyim/DeneyimSil/@item.ID" class="btn btn-danger">Sil</a>
          <td><a href="/Deneyim/DeneyimGetir/@item.ID" class="btn btn-warning">düzenle</a>

          </td>
        </tr>

    }

</table>

<a href="/Deneyim/DeneyimEkle" class="btn btn-primary">Yeni Deneyim Ekle</a>
