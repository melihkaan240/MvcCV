@model MvcCv.Models.Entity.TblSertifikalarım

@{
    ViewBag.Title = "SertifikaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4>Sertifika Düzenleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Düzenle</button>
    <a href="/Sertifikalar/SertifikaSil/@ViewBag.d" class="btn btn-danger"> Sil</a>
</form>


